<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
  <navigation-rule>
  	<from-view-id>../pages/menu.xhtml</from-view-id>
    <navigation-case>
		<from-outcome>menu</from-outcome>
		<to-view-id>../pages/menu.xhtml</to-view-id>
  	</navigation-case>
  </navigation-rule>    
  <managed-bean>
    <managed-bean-name>Session</managed-bean-name>
    <managed-bean-class>com.radev.foundation.security.SessionBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
</faces-config>
